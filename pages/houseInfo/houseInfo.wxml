<!--pages/product/product.wxml-->
<view class="" style="width:100%">
  <view style='margin-left:10rpx; padding-top:30rpx;'>
    <video id="myVideo" class="image_frame" src="{{house.video}}" controls>
    </video>
  </view>

  <view style='display: flex;flex-direction: row; margin-bottom:20rpx'>
    <view class='' style='padding-left: 30rpx;float: left;width: 700rpx;position: relative;'>
      <view class='flex-row' style='width:100%;'>
        <!-- 小区名 -->
        <view class='office-title-font' style='font-weight:bold;width:400rpx;margin-left:120px;margin-top:-180rpx;'>
          【 {{house.title}}】
        </view>
        <!-- 类型 -->
        <view class='office-font-price' style='margin-left:-400rpx;float:left;margin-top:-60px; color:yellowgreen;'>{{house.type}}</view>
        <view class='office-font-price' style='margin-left:30px;float:left;margin-top:-60px; color:yellowgreen;'>{{house.price}}</view>
      </view>
      <!-- 合约经纪人/意向用户/我的客户 -->

      <view style='color:#AAA;width:100%; margin-left:120px;margin-top:-38px;'>
        <view style="margin-left:10rpx;"> {{house.jingjiren}}</view>
        <view style="margin-left:0rpx;"> 合约经纪人</view>
      </view>
      <!-- 意向用户 -->
      <view style='color:#AAA;width:690rpx; margin-left:210px;margin-top:-38px;'>
        {{house.yonghu}}
        <view style="margin-left:-8px;"> 意向用户</view>
      </view>
      <!-- 我的客户 -->
      <view style='color:#AAA;width:690rpx; margin-left:280px;margin-top:-38px;'>
        {{house.kehu}}
        <view style="margin-left:-8px;"> 我的客户</view>
      </view>
    </view>

  </view>

  <view style='width: 100%;height: 10rpx; background: #f5f5f5;'></view>



  <!-- 致电置业顾问 -->
  <view style="width:100%;height:60px;">
    <!-- <view style="color:yellowgreen;margin-left:10px;margin-top:10px;">{{house.guwentel}} -->
    <view class="text-oneline margin-bottom-10 bar-font" style="display:flex;align-items:center;margin-left:20rpx;margin-top:20rpx;" bindtap="callPhonenum" data-phonenum="{{house.guwentel}}">
      <image src="/images/phone.png" class="opt-icon-sma" style="margin-left:10rpx;"></image>
      <text class="margin-left-30 bar-text">{{house.guwentel}}</text>
      <view style="margin-left:100rpx;margin-top:12rpx; color:yellowgreen;">{{house.guwenname}}</view>
    </view>
    <view style="color:#AAA;margin-left:10px;margin-top:30rpx;">{{house.content}}</view>
  </view>


  <!-- //// -->




  <!-- </view> -->

  <view style='width: 100%;height: 10rpx; background: #f5f5f5;'></view>
  <!-- 户型推荐及用户评论 -->
  <view style="width:100%;height:40px;margin-left:12px;margin-top:10px;" bindtap="jumpHuxingInfo" data-huxingid="{{house.id}}">
    <image class="image_frame" src="../../images/h1.png" style="width:25px;height:25px;" mode="aspectFill"></image>
    <view style="color:black;font-size:13px;margin-left:32px;margin-top:-25px;">户型推荐（{{house.huxing}}）</view>
  </view>
  <view style="width:100%;height:80rpx;margin-left:470rpx;margin-top:-39px;" bindtap="jumpCommentInfo" data-commentid="{{house.id}}">
    <image class="image_frame" src="../../images/p.png" style="width:25px;height:25px;" mode="aspectFill"></image>
    <view style="color:black;font-size:13px;margin-left:75rpx;margin-top:-25px;">用户评论（{{house.comment}}）</view>
  </view>
  <view style='width: 100%;height: 10rpx; background: #f5f5f5;'></view>


  <!-- 楼盘信息 -->
  <view style="width:100%;height:35px;margin-left:45rpx;margin-top:10px;">
    <view style="font-size:15px;">楼盘信息</view>
  </view>
  <view style='width: 100%;height: 5rpx; background: #f5f5f5;'></view>
  <view style="width:100%;height:35px;margin-left:12px;margin-top:10px;" bindtap="jumpOfficeInfo" data-officeid="{{house.id}}">
    <image class="image_frame" src="../../images/canshu.png" style="width:25px;height:25px;" mode="aspectFill"></image>
    <view style="margin-left:30px;margin-top:-30px;">楼盘参数
      <image src='../../images/进入.png' style='width:40rpx;height:40rpx;margin-left:500rpx;'></image>
    </view>
  </view>
  <view style='width: 100%;height: 5rpx; background: #f5f5f5;'></view>
  <!-- 楼盘位置 -->

  <view style="width:100%;height:35px;margin-left:12px;margin-top:10px;">
    <image class="image_frame" src="../../images/weizhi.png" style="width:25px;height:25px;" mode="aspectFill"></image>
    <view style="margin-left:30px;margin-top:-26px;">楼盘位置：{{house.address}}</view>
    <!-- <map id="myMap" show-location />

<button type="primary" bindtap="getCenterLocation">获取位置</button>
<button type="primary" bindtap="moveToLocation">移动位置</button>
<button type="primary" bindtap="translateMarker">移动标注</button>
<button type="primary" bindtap="includePoints">缩放视野展示所有经纬度</button> -->
  </view>
  <view style='width: 100%;height: 5rpx; background: #f5f5f5;'></view>

  <!-- 合作佣金 -->
  <view style="width:100%;height:35px;margin-left:12px;margin-top:10px;">
    <view style="font-size:15px;">合作佣金</view>
  </view>
  <view style='width: 100%;height: 5rpx; background: #f5f5f5;'></view>

  <view style="width:100%;height:30px;margin-left:12px;margin-top:10px;">
    <image class="image_frame" src="../../images/yong.png" style="width:25px;height:25px;" mode="aspectFill"></image>
    <view style="margin-left:30px;margin-top:-26px;">佣金：{{house.commission}}</view>
  </view>
  <view style='width: 100%;height: 5rpx; background: #f5f5f5;'></view>
  <view style="width:100%;height:35px;margin-left:12px;margin-top:10px;">
    <image class="image_frame" src="../../images/jiang.png" style="width:25px;height:25px;" mode="aspectFill"></image>
    <view style="margin-left:30px;margin-top:-26px;">成交奖励：{{house.reward}}</view>
  </view>

  <view style='width: 100%;height: 5rpx; background: #f5f5f5;'></view>
  <view style="width:100%;height:35px;margin-left:12px;margin-top:10px;">
    <image class="image_frame" src="../../images/shi.png" style="width:25px;height:25px;" mode="aspectFill"></image>
    <view style="margin-left:30px;margin-top:-26px;">结算周期：{{house.period}}</view>
  </view>

  <!-- 报备客户 -->
  <view class="page__bd" style="">

    <navigator url='' bindtap='jumpBaobei' class="weui-grid-grating" hover-class="weui-grid_active" style="background-color:yellowgreen;width:100%;height:100rpx;" data-myType='2' data-house_id="{{house.id}}" data-house_title="{{house.title}}" data-visitingstates="未到访">
      <!-- <view style="padding-left:30px;padding-top:30px;"> 报备客户</view> -->
      <!-- <image class="weui-grid__icon-copy" src="../../images/intro.png" mode="scaleToFill" /> -->
      <view class="weui-grid__label" style=" font-size:20px;color:white;padding-top:6px;">报备客户</view>
    </navigator>

  </view>
</view>