<!--pages/houseInfo/detail/detail.wxml-->

<view class="{{hidden}}" style="height:100%;;">
  <!-- 楼盘参数 -->
  <view style="height:30px;margin-left:0px;margin-top:0px;">
    <!-- <view style="font-size:15px;">基本参数</view> -->
    <image src="/images/jb2.png" style="width:94%;height:90rpx;margin-left:22rpx;" />
  </view>
  <view style='height: 3rpx; background: #f5f5f5;'></view>
  <view style="height:20px; margin-left:70rpx;margin-top:20px;">开盘时间：
    <view style="margin-left:80px;margin-top:-34.4rpx;">{{HouseDetail.kaipantime}}</view>
  </view>
  <image src="/images/canshu-05.png" style="width:100%;height:3rpx;"> </image>
  <view style="height:25px; margin-left:70rpx;margin-top:8px;">交盘时间：
    <view style="margin-left:80px;margin-top:-34.4rpx;">{{HouseDetail.jiaopantime}}</view>
  </view>
  <image src="/images/canshu-05.png" style="width:100%;height:3rpx;"> </image>
  <view style="height:20px; margin-left:70rpx;margin-top:8px;">开发商：
    <view style="margin-left:80px;margin-top:-34.4rpx;">{{HouseDetail.developer}}</view>
  </view>
  <image src="/images/canshu-05.png" style="width:100%;height:3rpx;"> </image>
  <view style="height:20px; margin-left:70rpx;margin-top:8px;">物业公司：
    <view style="margin-left:80px;margin-top:-34.4rpx;">{{HouseDetail.property}}</view>
  </view>
  <image src="/images/canshu-05.png" style="width:100%;height:3rpx;"> </image>
  <view style="height:20px; margin-left:70rpx;margin-top:8px;">建筑面积：{{HouseDetail.size}}
    <view style="height:20px; margin-left:165px;margin-top:-19px;">总户数：{{HouseDetail.households}}</view>
  </view>
  <image src="/images/canshu-05.png" style="width:100%;height:3rpx;"> </image>
  <view style="height:20px; margin-left:70rpx;margin-top:8px;">容积率：{{HouseDetail.plotratio}}
    <view style="height:20px; margin-left:165px;margin-top:-19px;">绿化率：{{HouseDetail.green}}</view>
  </view>
  <image src="/images/canshu-05.png" style="width:100%;height:3rpx;"> </image>
  <view style="height:20px; margin-left:70rpx;margin-top:8px;">车位数：{{HouseDetail.park}}
    <view style=" margin-left:165px;margin-top:-19px;">车位比：{{HouseDetail.parkper}}</view>
  </view>
  <image src="/images/canshu-05.png" style="width:100%;height:3rpx;"> </image>
  <!-- 住宅参数 -->
  <!-- <view style="font-size:15px;">住宅参数</view> -->
  <image src="/images/zz2.png" style="width:94%;height:90rpx;margin-left:22rpx;margin-top:-6rpx;" />

  <view style="height:20px; margin-left:70rpx;margin-top:0px;">均价：
    <view style="margin-left:80px;margin-top:-34.4rpx;">{{HouseDetail.price}}</view>
  </view>
  <image src="/images/canshu-05.png" style="width:100%;height:3rpx;"> </image>
  <view style="height:20px; margin-left:70rpx;margin-top:8px;">物业费：
    <view style="margin-left:80px;margin-top:-34.4rpx;">{{HouseDetail.propertyfee}}</view>
  </view>
  <image src="/images/canshu-05.png" style="width:100%;height:3rpx;"> </image>
  <view style="height:20px; margin-left:70rpx;margin-top:8px;">建筑类型：
    <view style="margin-left:80px;margin-top:-34.4rpx;">{{HouseDetail.buildtype}}</view>
  </view>
  <image src="/images/canshu-05.png" style="width:100%;height:3rpx;"> </image>
  <view style="height:20px; margin-left:70rpx;margin-top:8px;">装修状态：
    <view style="margin-left:80px;margin-top:-34.4rpx;">{{HouseDetail.decorate}}</view>
  </view>
  <image src="/images/canshu-05.png" style="width:100%;height:3rpx;"> </image>
  <view style="height:20px; margin-left:70rpx;margin-top:8px;" data-officeid="{{id}}">产权年限：
    <view style="margin-left:80px;margin-top:-34.4rpx;">{{HouseDetail.years}}</view>
  </view>
  <image src="/images/canshu-05.png" style="width:100%;height:3rpx;"> </image>
  <!-- 周边配套 -->
  <view style="height:30px;margin-left:0px;margin-top:0px;">
    <!-- <view style="font-size:15px;">周边配套</view> -->
    <image src="/images/zb2.png" style="width:94%;height:90rpx;margin-left:22rpx;" />
  </view>
  <image src="/images/canshu-05.png" style="width:100%;height:3rpx;"> </image>
  <view style="width:86%;height:90px; margin-left:70rpx;margin-top:15px;">商业配套：
    <view style="margin-left:80px;margin-top:-34.4rpx;">{{HouseDetail.shangye}}</view>
  </view>
  <image src="/images/canshu-05.png" style="width:100%;height:3rpx;"> </image>
  <view style="width:86%;height:90px; margin-left:70rpx;margin-top:8px;">教育配套：
    <view style="margin-left:80px;margin-top:-34.4rpx;">{{HouseDetail.jiaoyu}}</view>
  </view>
  <image src="/images/canshu-05.png" style="width:100%;height:3rpx;"> </image>
  <view style="width:87%;height:80px; margin-left:70rpx;margin-top:8px;">交通配套：
    <view style="margin-left:80px;margin-top:-34.4rpx;">{{HouseDetail.jiaotong}}</view>
  </view>
  <image src="/images/canshu-05.png" style="width:100%;height:3rpx;"> </image>
  <view style="width:87%;height:90px; margin-left:70rpx;margin-top:8px;">环境配套：
    <view style="margin-left:80px;margin-top:-34.4rpx;">{{HouseDetail.huanjing}}</view>
  </view>
</view>